public class Main {
public static void main(String args[]) {
int x=+56;
System.out.println(x);
System.out.println(-x);  
System.out.println(x++);   
System.out.println(++x); 
System.out.println(x--);
System.out.println(--x);  
int a=38;
int b=-65;
boolean c=true;
boolean d=false;
System.out.println(~a);
System.out.println(~b);
System.out.println(!c);
System.out.println(!d);
}
}
